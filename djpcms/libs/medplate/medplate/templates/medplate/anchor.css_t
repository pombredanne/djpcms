{% extends "medplate/elem.css_t" %}

{% block body %}{% if color %}
{{ tag }}:link,
{{ tag }}:visited {
    color: {{ color }};
}{% endif %}
{{ tag }}:hover { {% if color_hover %}
    color: {{ color_hover }};{% endif %}{% if background_hover %}
    background: {{ background_hover }}{% endif %}
}
{% endblock %}