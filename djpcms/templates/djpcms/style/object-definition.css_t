{% extends "medplate/elem.css_t" %}

{% block body %}

{{ tag }} {
	margin-bottom: 10px;
}
{{ tag }} dl {
	overflow: hidden;
	margin: 5px 0 0; 
}
{{ tag }} dl:first-child {
	margin-top:0;
}
{{ tag }} dl dt {
	opacity: 0.6;
	float: left;
	width: {{ left_width }};
	margin: 0;
}
{{ tag }} dl dd {
	margin: 0;
}
{{ tag }} dl dd textarea {
    border: none;
    width: {{ right_width }};
}
{{ tag }}.w40 dl dt {
	width: 40%;
}
{{ tag }}.w50 dl dt {
	width: 50%;
}
{% endblock %}